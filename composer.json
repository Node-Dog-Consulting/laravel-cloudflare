{
  "name": "joshuapack/laravel-cloudflare",
  "description": "Use the Cloudflare API right from Laravel.",
  "homepage": "https://github.com/joshuapack/laravel-cloudflare",
  "keywords": [
    "cloudflare",
    "laravel",
    "api"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Joshuapack",
      "email": "joshua@joshuapack.com",
      "homepage": "https://joshuapack.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "cloudflare/sdk": "^1.1",
    "illuminate/contracts": "^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Joshuapack\\Cloudflare\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "aliases":{
        "Cloudflare": "Joshuapack\\Cloudflare\\CloudflareFacade"
      },
      "providers": [
        "Joshuapack\\Cloudflare\\CloudflareServiceProvider"
      ]
    }
  }
}
